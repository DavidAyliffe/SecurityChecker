# Copy this file to .env and fill in your real values.
# .env is git-ignored; .env.example is safe to commit.

# Full JDBC connection URL for your MySQL server.
# Format: jdbc:mysql://<host>:<port>/<database>
DB_URL=jdbc:mysql://localhost:3306/usernamepassword

# MySQL username
DB_USER=root

# MySQL password
DB_PASSWORD=your_password_here
